      "value": "\".+?\""
    },
    "literal_nat": {
      "type": "PATTERN",
      "value": "[0-9]{1,20}"
    },
    "literal_int": {
      "type": "PATTERN",
      "value": "[+-][0-9]+"
    },
    "literal_float": {
      "type": "PATTERN",
      "value": "[+-]?[0-9]*\\.[0-9]+"
    },
    "literal_char": {
      "type": "PATTERN",
      "value": "\\?."
    },
    "literal_boolean": {
      "type": "CHOICE",
